    "slug": "rocket-help-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo/logo-playstore.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/pre-render/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
